rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /demos/{demoId} {
      allow read: if true;
      allow write: if request.auth != null;
      
      match /mockup_appointments/{appId} {
        allow read, write: if true;
      }
    }
    
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}